#ifdef GL_ES
#define LOWP lowp
precision mediump float;
#else
#define LOWP
#endif
varying LOWP vec4 v_color;
varying vec2 v_texCoords;
uniform float iGlobalTime;
uniform sampler2D u_texture;
uniform sampler2D u_texture2;
void main()
{
  gl_FragColor = v_color
                    * ((texture2D(u_texture, vec2(v_texCoords.x, v_texCoords.y)) * vec4(0.5,0.5,0.5,0.5))
                    + (texture2D(u_texture2, vec2(v_texCoords.x, v_texCoords.y)) * vec4(0.5,0.5,0.5,0.5)));
}
